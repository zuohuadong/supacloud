{
    "compilerOptions": {
        "lib": [
            "ESNext",
            "DOM"
        ],
        "target": "ESNext",
        "module": "ESNext",
        "moduleDetection": "force",
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx",
        "allowImportingTsExtensions": true,
        "moduleResolution": "bundler",
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true
    }
}